{
  "name": "kampfcaspar/filter",
  "description": "Filter abstraction layer for PHP",
  "homepage": "https://github.com/KampfCaspar/Filter",
  "license": "AGPL-3.0-or-later",
  "type": "library",
  "require": {
    "php": "^8.2",
    "psr/log": "^3.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^1.10",
    "phpstan/extension-installer": "^1.3",
    "phpstan/phpstan-phpunit": "^1.3",
    "struggle-for-php/sfp-phpstan-psr-log": "^0.17",
    "beste/psr-testlogger": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "KampfCaspar\\Filter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KampfCaspar\\Test\\Filter\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "KampfCaspar",
      "email": "code@kampfcaspar.ch",
      "role": "Developer"
    }
  ],
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
